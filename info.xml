<?xml version="1.0"?>
<extension key="eu.businessandcode.dashletnote" type="module">
  <file>dashletnote</file>
  <name>Dashlet Note</name>
  <description>Let's you create a custom dashlet with free text.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Alain Benbassat</author>
    <email>alain@businessandcode.eu</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/AlainBenbassat/eu.businessandcode.dashletnote</url>
    <url desc="Documentation">https://github.com/AlainBenbassat/eu.businessandcode.dashletnote</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2021-06-07</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>See admin menu or civicrm/dashlet-note/admin to manage your dashlet.</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Dashletnote</namespace>
  </civix>
</extension>
